    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/toolbar_with_article"/>

        <TextView
            android:id="@+id/activity_developkit_butterknife_tv_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/darkgrey" />

        <TextView
            android:id="@+id/activity_developkit_butterknife_tv_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/darkgrey" />

        <TextView
            android:id="@+id/activity_developkit_butterknife_tv_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/darkgrey" />

        <TextView
            android:id="@+id/activity_developkit_butterknife_tv_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/darkgrey" />
    </LinearLayout>
</ScrollView>
